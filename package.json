{
  "name": "dtalk",
  "version": "3.8.8",
  "description": "钉钉-Linux版",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder"
  },
  "author": {
    "name": "yutent",
    "email": "yutent@doui.cc"
  },
  "devDependencies": {
    "electron": "^9.1.0",
    "electron-builder": "^21.2.0"
  },
  "build": {
    "appId": "cc.doui.dtalk",
    "productName": "DingDing",
    "copyright": "Copyright © 2019 ${author}",
    "directories": {
      "buildResources": "icons",
      "output": "build"
    },
    "files": ["src/**/*"],
    "linux": {
      "category": "Network;Chat",
      "target": [
        {
          "target": "deb",
          "arch": "x64"
        },
        {
          "target": "AppImage",
          "arch": "x64"
        }
      ],
      "icon": "./icons/"
    }
  },
  "dependencies": {}
}
